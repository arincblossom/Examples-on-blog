<?xml version="1.0" encoding="utf-8"?>
<manifest
	xmlns:android="http://schemas.android.com/apk/res/android"
        package="com.example.package">
         
	 <!-- 안드로이드 버전 제한 -->
	<uses-sdk android:minSdkVersion="14"/>	<!-- 최소 사용 가능 버전 -->
	<uses-sdk android:targetSdkVersion="19"/>	<!-- 컴파일 하는 버전 -->
	<uses-sdk android:maxSdkVersion="23"/>	<!-- 최대 사용 가능 버전. 지원이 되어도 이 이상의 버전에선 실행불가 -->
	
	<!-- 안드로이드 퍼미션 -->
	<uses-permission android:name="android.permission.[ 적용할 퍼미션 이름 ]"/>
	<!-- 퍼미션 리스트는 안드로이드 개발자 페이지 https://developer.android.com/reference/android/Manifest.permission.html 에서 확인 -->
	
	<!-- 앱/액티비티 설정 -->
	<application
		android:allowBackup="true"
		android:icon="@drawable/app_icon"
		android:label="@string/app_name"
		android:supportsRtl="true"
		android:theme="@style/AppTheme">
		<activity android:name=".MainActivity">
			<intent-filter>
				<action android:name="android.intent.action.MAIN"/>
				<category android:name="android.intent.category.LAUNCHER"/>
			</intent-filter>
		</activity>
	</application>

</manifest>
